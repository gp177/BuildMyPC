@model BuildMyPC.Models.PowerSupply
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3>@Model.Name</h3>
<ul>
    <li>Brand: @Model.Brand.BrandName</li>
    <li>Efficiency: @Model.Efficiency GHz</li>
    <li>Watts: @Model.Watts W</li>
    <li>Is Modular? @Model.IsModular </li>
    <li>Price: $@Model.Price</li>
</ul>

@Html.ActionLink("Back to List", "Index", "PowerSupply")


